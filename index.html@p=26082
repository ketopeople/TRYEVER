#main-header .et_mobile_menu .menu-item-has-children > a:after { font-family: 'FontAwesome'; text-align: center; speak: none; font-weight: bold; font-variant: normal; text-transform: none; -webkit-font-smoothing: antialiased; position: absolute; }
